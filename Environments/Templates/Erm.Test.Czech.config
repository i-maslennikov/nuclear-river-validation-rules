<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <appSettings>
    <add key="JobStoreType" value="TX"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="TargetEnvironmentName" value="Test.{EnvNum}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="IntegrationApplicationName" value="ERM{EnvNum}.Search"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SchedulerName" value="ReplicationService.Scheduler.Test.{EnvNum}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="TransportEntityPath" value="{DestTopic}"
         xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>

  <connectionStrings>
    <add name="Erm" connectionString="Data Source=uk-erm-sql02;Initial Catalog=Erm{EnvNum};Integrated Security=True;Enlist=False;"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
    <add name="Facts" connectionString="Data Source=uk-erm-sql02;Initial Catalog=CustomerIntelligence{EnvNum};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="CustomerIntelligence" connectionString="Data Source=uk-erm-sql02;Initial Catalog=CustomerIntelligence{EnvNum};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Transport" connectionString="Data Source=uk-erm-sql02;Initial Catalog=CustomerIntelligence{EnvNum};Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Logging" connectionString="http://logstash.prod.erm.2gis.ru:8194"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="Infrastructure" connectionString="Data Source=uk-erm-sql02;Initial Catalog=ErmInfrastructure;Integrated Security=True"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="ServiceBus" connectionString="Endpoint=sb://uk-sql01.2gis.local/ERM-Test{EnvNum},sb://uk-erm-sql02.2gis.local/ERM-Test{EnvNum};StsEndpoint=https://uk-sql01.2gis.local:9355/ERM-Test{EnvNum},https://uk-erm-sql02.2gis.local:9355/ERM-Test{EnvNum};RuntimePort=9354;ManagementPort=9355;OperationTimeout=00:00:05"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>

</configuration>